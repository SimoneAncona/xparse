{
    "$schema": "https://raw.githubusercontent.com/SimoneAncona/xparser/main/schemas/schema.json",
    "name": "MyGrammar",
    "atomic": [
        {
            "name": "string",
            "regex": "\".*\""
        }
    ],
    "rules": [
        {
            "name": "stringDefinition",
            "expressions": [
                "def <string><space?>;"
            ]
        }
    ]
}